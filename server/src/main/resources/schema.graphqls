type People {
  page: String!
  next: String!
  previous: String
  allResults: [AllResults]!
}
type AllResults {
  name: String!
  height: Int
  mass: String
  gender: String!
  homeworld: String!
}
type Person {
  Results: [Results]
}
type Results {
  name: String
  height: Int
  mass: String
  gender: String
  homeworld: String
}

type Query {
  people(page: String!): People
  person(name: String): Person
}
